<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/historico_data_tv"
        style="@style/textViewLayoutLabelDefault"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margem_pequena"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="30/01/2023"/>

    <TextView
        android:id="@+id/historico_descricao_tv"
        style="@style/textViewLayoutDefault"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margem_pequena"
        android:layout_marginStart="@dimen/margem_padrao"
        android:layout_marginBottom="@dimen/margem_pequena"
        android:lineBreakStyle="normal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/historico_data_tv"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:text="Teste Descricao Projeto" />

    <com.google.android.material.divider.MaterialDivider
        android:id="@+id/equipe_participante_separador"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margem_pequena"
        app:dividerThickness="0.5dp"
        app:dividerColor="@color/secondary_color"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/historico_descricao_tv"/>


</androidx.constraintlayout.widget.ConstraintLayout>